	<!--
		The three type of status are:

	LOGIN_SUCCESS = 0;
	LOGIN_FAILED_CLIENT = 1;
	LOGIN_FAILED_SERVER = 2;
	PLAYER_SPAWN = 3;
	LOGIN_FAILED_INVALID_TENANT = 4;
	LOGIN_FAILED_VANILLA_EDU = 5;
	LOGIN_FAILED_EDU_VANILLA = 6;
		-->
